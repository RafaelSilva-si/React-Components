		"react-simple-maps": "^0.12.1",
		"react-sizeme": "^2.5.2",
		"react-spinners": "^0.8.0",
		"react-tiny-fab": "^3.4.0",
		"reactstrap": "^7.1.0",
		"redux": "^4.0.5",
		"redux-saga": "^1.1.3",
		"styled-components": "^5.0.0",
		"yup": "^0.28.5"
	},
	"devDependencies": {
		"prettier": "^1.19.1",
		"react-scripts": "^3.4.0",
		"sass": "^1.45.0",
		"serve": "^10.1.2"
	},
	"scripts": {
		"build": "yarn build:production",
		"build:development": "env-cmd -f .env.development react-scripts build",
		"build:production": "env-cmd -f .env.development react-scripts build",
		"colors": "node src/lib/scripts/colors.js",
		"deploy": "gh-pages -d build",
		"eject": "react-scripts eject",
		"format": "prettier --write \"**/*.+(js|jsx|json|css|md)\"",
		"images": "node src/lib/scripts/images.js",
		"serve": "serve -s build",
		"start": "yarn start:development",
		"start:development": "env-cmd -f .env.development react-scripts start",
		"start:production": "env-cmd -f .env.development react-scripts start",
		"test": "yarn test:staging",
		"test:development": "env-cmd -f .env.production react-scripts test --env=jsdom",
		"test:production": "env-cmd -f .env.production react-scripts test --env=jsdom"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged && echo All Doned"
		}
	}
}
